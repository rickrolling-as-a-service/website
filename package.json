{
	"name": "raas",
	"description": "Rickrolling as a service.",
	"scripts": {
		"start": "next start",
		"build": "next build",
		"dev": "next",
		"dev:api": "nodemon api.js",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"export": "next export"
	},
	"version": "1.0.0",
	"main": "pages/index.js",
	"author": "Felix Mattick",
	"license": "UNLICENSED",
	"private": false,
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"rules": {
			"react/react-in-jsx-scope": "off",
			"semi": [
				"error",
				"never"
			],
			"quotes": [
				"error",
				"single"
			]
		},
		"parser": "babel-eslint",
		"env": {
			"node": true,
			"browser": true,
			"es6": true
		}
	},
	"dependencies": {
		"@zeit/next-sass": "^1.0.1",
		"bcrypt-nodejs": "^0.0.3",
		"bootstrap": "^4.3.1",
		"express": "^4.16.4",
		"mongoose": "^5.4.13",
		"next": "^8.0.1",
		"next-seo": "^1.11.1",
		"node-fetch": "^2.3.0",
		"node-sass": "^4.11.0",
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"react-runkit": "^0.9.0",
		"shortid": "^2.2.14",
		"uuid": "^3.3.2",
		"webpack": "^4.29.4"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.14.0",
		"eslint-plugin-react": "^7.12.4",
		"nodemon": "^1.18.10"
	}
}
